{
  "manifest_version": 2,

  "name": "Codesmith Extension",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
    {
      "css": ["styles.css"],
      "js": ["thirdParty/jquery.1.10.2.min.js", "moment.js", "main.js"]
    }
  ]
}
